      \verb /home/andrea/Zotero/storage/CG7ZVBT7/Pfaff et al. - 2021 - Learning Mesh-Based Simulation with Graph Networks.pdf;/home/andrea/Zotero/storage/FXUGV7BX/2010.html
      \endverb
      \keyw{Computer Science - Computational Engineering Finance and Science,Computer Science - Machine Learning}
