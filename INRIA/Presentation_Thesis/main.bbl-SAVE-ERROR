    \entry{pfaffLearningMeshBasedSimulation2021a}{misc}{}
      \name{author}{4}{}{%
        {{hash=a726408819b6f955652c3ffaaedef966}{%
           family={Pfaff},
           familyi={P\bibinitperiod},
           given={Tobias},
